#
# TABLE STRUCTURE FOR: creditos
#

DROP TABLE IF EXISTS `creditos`;

CREATE TABLE `creditos` (
  `ID` int(11) NOT NULL AUTO_INCREMENT,
  `CREDITO` text,
  `SALDO` double DEFAULT NULL,
  `FECHA_VEN` int(2) DEFAULT NULL,
  `TOTAL_CUOTAS` int(3) DEFAULT '0',
  `CUOTAS_PAGAS` int(3) DEFAULT '0',
  `INTERES` double DEFAULT NULL,
  `ULTIMO_PAGO` varchar(11) DEFAULT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=latin1;

INSERT INTO `creditos` (`ID`, `CREDITO`, `SALDO`, `FECHA_VEN`, `TOTAL_CUOTAS`, `CUOTAS_PAGAS`, `INTERES`, `ULTIMO_PAGO`) VALUES ('3', 'ICETEX', '7984630', '10', '55', '1', '0', '2017-07-15');
